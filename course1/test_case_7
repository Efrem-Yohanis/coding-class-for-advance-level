import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://13f-qa.azurewebsites.net/');
  await page.getByRole('button', { name: 'Sign in with DFIN Account' }).click();
  await page.getByRole('button', { name: 'ABS-EE Deal Home' }).click();
  await page.getByRole('heading', { name: 'ABS-EE Deal Home' }).click();
  await page.getByRole('columnheader', { name: 'Deal Name sort asc' }).locator('span').click();
  await page.getByRole('link', { name: '2' }).click();
  await page.getByRole('link', { name: '1' }).click();
  await page.getByRole('columnheader', { name: 'Deal Name sort desc' }).locator('span').click();
  await page.getByRole('columnheader', { name: 'Deal Name sort desc' }).locator('span').click();
  await page.getByRole('columnheader', { name: 'Deal Name sort desc' }).locator('span').click();
  await page.getByRole('columnheader', { name: 'Deal Name sort asc' }).locator('span').click();
  await page.getByRole('row', { name: '99 submission proof to be' }).getByRole('link').click();
  await page.getByText('Submission Proof').click();
  await page.getByRole('button', { name: 'Create' }).click();
  await page.getByRole('button', { name: 'No' }).click();
  await page.getByText('File Name:').click();
  await page.getByText('99.html').click();
  await page.getByText('Created Date:').nth(1).click();
  await page.getByText('-11-2026, 5:00:24 PM EST').click();
});
